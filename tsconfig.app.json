{
  "extends": "./tsconfig.json",
  "include": [
    "app/**/*.ts",
    "app/**/*.tsx",
    "components/**/*.ts",
    "components/**/*.tsx",
    "constants/**/*.ts",
    "constants/**/*.tsx",
    "contexts/**/*.ts",
    "contexts/**/*.tsx",
    "data/**/*.ts",
    "data/**/*.tsx",
    "features/**/*.ts",
    "features/**/*.tsx",
    "hooks/**/*.ts",
    "hooks/**/*.tsx",
    "services/**/*.ts",
    "services/**/*.tsx",
    "src/**/*.ts",
    "src/**/*.tsx",
    "types/**/*.ts",
    "types/**/*.tsx",
    "utils/**/*.ts",
    "utils/**/*.tsx",
    "expo-env.d.ts"
  ],
  "exclude": [
    "dist/**",
    "docs/**",
    "node_modules/**",
    "reference/**",
    "scripts/**",
    "supabase/**",
    "tools/**",
    ".expo/types/**",
    "**/*.test.ts",
    "**/*.test.tsx",
    "test-*.ts",
    "test-*.tsx",
    "CACHE_INTEGRATION_EXAMPLES.tsx",
    "PRAYER_GUIDANCE_USAGE_EXAMPLES.tsx",
    "**/*.example.ts",
    "**/*.example.tsx",
    "**/*Examples.tsx",
    "**/*-old.ts",
    "**/*-old.tsx"
  ]
}
